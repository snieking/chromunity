import election;
import user;

entity representative {
    key user.user, election.representative_election;
}

entity representative_distrust {
	key distruster: representative, distrusted: representative;
}

entity representative_action {
    key id: byte_array;
    index representative;
    index timestamp;
    action: text;
}

entity representative_action_vote {
	key id: text, representative;
}

import representative.report;
